{
  "name": "gphoto",
  "version": "1.0.0",
  "author": "Jack Cannon <jackc@annon.co.uk> (http://c.annon.co.uk/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackcannon/gphoto.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "tsup": "^6.2.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "yarn build:dist && yarn build:docs",
    "build:dist": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "build:docs": "typedoc --plugin typedoc-plugin-markdown --entryDocument 'API.md' --readme none --out docs src/index.ts",
    "watch": "yarn build:dist -- --watch src",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "swiss-ak": "^1.13.0"
  }
}
